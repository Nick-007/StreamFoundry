{
  "$schema": "https://schemas.streamfoundry.dev/appsettings-template/v1.json",
  "appSettings": {
    "FUNCTIONS_WORKER_RUNTIME": {
      "default": "python",
      "description": "Azure Functions language worker runtime."
    },
    "AzureWebJobsStorage": {
      "type": "keyVault",
      "secretName": "azure-webjobs-storage",
      "description": "Primary storage connection string used by the Functions host."
    },
    "BASE_URL": {
      "default": "https://<storage-account>.blob.core.windows.net",
      "description": "Base URL for blob outputs."
    },
    "DASH_BASE_URL": {
      "default": "https://<storage-account>.blob.core.windows.net/dash"
    },
    "HLS_BASE_URL": {
      "default": "https://<storage-account>.blob.core.windows.net/hls"
    },
    "RAW_CONTAINER": {
      "default": "raw"
    },
    "MEZZ_CONTAINER": {
      "default": "mezzanine"
    },
    "SKIP_TRANSCODE_IF_MEZZ": {
      "default": "true"
    },
    "SKIP_MEZZ_UPLOAD_ON_RESTORE": {
      "default": "true"
    },
    "HLS_CONTAINER": {
      "default": "hls"
    },
    "DASH_CONTAINER": {
      "default": "dash"
    },
    "LOGS_CONTAINER": {
      "default": "logs"
    },
    "PROCESSED_CONTAINER": {
      "default": "processed"
    },
    "TMP_DIR": {
      "default": "/tmp/ingestor"
    },
    "TRANSCODE_QUEUE": {
      "default": "transcode-jobs"
    },
    "TRANSCODE_POISON_QUEUE": {
      "default": "transcode-jobs-poison"
    },
    "PACKAGING_QUEUE": {
      "default": "packaging-jobs"
    },
    "FFMPEG_PATH": {
      "default": "ffmpeg"
    },
    "FFPROBE_PATH": {
      "default": "ffprobe"
    },
    "SHAKA_PACKAGER_PATH": {
      "default": "packager"
    },
    "SEG_DUR_SEC": {
      "default": "4"
    },
    "PACKAGER_SEG_DUR_SEC": {
      "default": "4"
    },
    "LADDER_PROFILE": {
      "default": "efficient"
    },
    "LOCKS_CONTAINER": {
      "default": "locks"
    },
    "LOCK_TTL_SECONDS": {
      "default": "60"
    },
    "VIDEO_CODEC": {
      "default": "h264_nvenc"
    },
    "NVENC_PRESET": {
      "default": "p5"
    },
    "NVENC_RC": {
      "default": "vbr_hq"
    },
    "NVENC_LOOKAHEAD": {
      "default": "32"
    },
    "NVENC_AQ": {
      "default": "1"
    },
    "SET_BT709_TAGS": {
      "default": "true"
    },
    "AUDIO_MAIN_KBPS": {
      "default": "128"
    },
    "ENABLE_AUDIO_LOW": {
      "default": "false"
    },
    "ENABLE_CAPTIONS": {
      "default": "true"
    },
    "ENABLE_TRICKPLAY": {
      "default": "true"
    },
    "TRICKPLAY_FACTOR": {
      "default": "4"
    },
    "THUMB_INTERVAL_SEC": {
      "default": "4"
    },
    "QC_STRICT": {
      "default": "true"
    },
    "DRM_PLACEHOLDERS": {
      "default": "true"
    },
    "TRANSCODE_VISIBILITY_EXTENSION_SEC": {
      "default": "300"
    },
    "PIPELINE_ROUTES": {
      "default": ""
    }
  }
}
