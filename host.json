{
  "version": "2.0",
  "queues": {
    "maxDequeueCount": 3,
    "visibilityTimeout": "00:02:00",
    "batchSize": 1,
    "newBatchThreshold": 0
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Warning",
      "Function.queue_ingestor": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}