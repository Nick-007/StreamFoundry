{
  "version": "2.0",
  "functionTimeout": "01:00:00",
  "extensions": {
    "queues": {
      "maxDequeueCount": 3,
      "visibilityTimeout": "00:10:00",
      "maxPollingInterval": "00:02:00",
      "batchSize": 1,
      "newBatchThreshold": 0
    },
    "blobs": {
      "maxDegreeOfParallelism": 1,
      "pollingInterval": "00:02:00",
      "maxBatchSize": 1
    },
    "http": {
      "routePrefix": "api",
      "maxOutstandingRequests": 200,
      "maxConcurrentRequests": 100,
      "dynamicThrottlesEnabled": true
    }
  },
  "logging": {
    "applicationInsights": {
      "enableLiveMetrics": true,
      "samplingSettings": {
        "isEnabled": false
      }
    },
    "logLevel": {
      "submit": "Debug",
      "transcode": "Debug", 
      "package": "Debug",
      "Function": "Debug",
      "Host.Results": "Debug",
      "Host.Triggers.Queue": "Debug",
      "Host.Triggers.Blob": "Debug",
      "Azure.Storage": "Debug",
      "Azure.Storage.Queues": "Debug",
      "Azure.Storage.Blobs": "Debug",
      "Azure.Core": "Debug",
      "Azure.Core.Http": "Debug",
      "Worker": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Azure.WebJobs": "Debug",
      "Microsoft.Azure.WebJobs.Host": "Debug",
      "Microsoft.Azure.WebJobs.Script": "Debug",
      "System.Net.Http": "Debug",
      "Default": "Debug"
    },
    "fileLoggingMode": "always"
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.26.2, 4.26.2]",
    "downloadPath": "$HOME/.azure-functions-extension-bundles"
  }
}