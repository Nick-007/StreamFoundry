{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxDequeueCount": 3,
      "visibilityTimeout": "00:02:00",
      "maxPollingInterval": "00:01:00",
      "batchSize": 1,
      "newBatchThreshold": 0
    },
    "blobs": {
      "maxDegreeOfParallelism": 1,
      "pollingInterval": "00:01:00"
    }
  },
  "logging": {
    "applicationInsights": {
      "enableLiveMetrics": true,
      "samplingSettings": {
        "isEnabled": false
      }
    },
    "logLevel": {
      "queue_ingestor": "Information",
      "Function": "Information",
      "Function.queue_ingestor": "Information",
      "Host.Results": "Information",
      "Host.Triggers.Queue": "Warning",
      "Azure.Storage": "Warning",
      "Azure.Storage.Queues": "Warning",
      "Azure.Core": "Warning",
      "Worker": "Warning",
      "Microsoft": "Warning",
      "Default": "Warning"
    },
    "fileLoggingMode": "always"
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.26.2, 4.26.2]",
    "downloadPath": "$HOME/.azure-functions-extension-bundles"
  }
}